/* pages/MyClub/MyClub.wxss*/
.ClubDetail{
  width: 100%;
  height: 100px;
  display: flex;
  border:1px solid rgba(10, 1, 1, 0.034);
}

.url{
  width: 100px;
  height: 100px; 
  background-color: #eeeeee;
}
.txt_Club{
 width: 100%-100px;
 height: 100px;
 display: block;
}
.txt_Club text{
 font-size: 14pt;
}
.main{
  width:100%;
  height:100%;
}
page,.page { 
  height: 100%; 
  font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif; 
} 

.page-bottom{ 
  height: 100%; 
  width: 750rpx; 
  position: fixed;  
  top: 0;
  bottom: 0;
  background-color: rgb(183, 193, 197); 
  z-index: 0; 
  overflow-y: scroll;
  overflow-x: hidden;
} 
.wc{ 
  color: white; 
  padding: 30rpx 0 30rpx 40rpx; 
} 


.page-top{ 
  height: 100%; 
  position: fixed;
  width: 750rpx; 
  top: 0;
  bottom: 0;
  background-color: rgb(239, 243, 248); 
  z-index: 0; 
  transition: All 0.4s ease;  
  -webkit-transition: All 0.4s ease; 
  display: block;
  overflow-y: scroll;
  overflow-x: hidden;
} 



.c-state1{ 
  transform: rotate(0deg) scale(1) translate(75%,0%);  
  -webkit-transform: rotate(0deg) scale(1) translate(75%,0%);  
} 
.c-state2{ 
  transform: rotate(0deg) scale(.8) translate(75%,0%);  
  -webkit-transform: rotate(0deg) scale(.8) translate(75%,0%);  
} 